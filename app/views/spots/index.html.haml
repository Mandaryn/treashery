= javascript_include_tag "http://maps.google.com/maps/api/js?v=3.4&sensor=false"
= javascript_include_tag "google_map.js"
#map_content
	#google_map
#spotlist
	%h3 Spot list:
	- @spots.each do |spot|
		.spot.vcard
			= link_to spot.name, spot, class: 'fn url'
			.geo.hidden
				.latitude
					= spot.lat
				.longitude
					= spot.lng
#add_new_spot
	%p= link_to 'New Spot', new_spot_path